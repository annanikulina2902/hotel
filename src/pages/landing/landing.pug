include ../../modules/header/header.pug
include ../../modules/footer/footer.pug
include ../../modules/submenu/submenu.pug


doctype html
head 
    meta(charset='UTF-8')
    meta(name="viewport", content="width=device-width, initial-scale=1.0")
    link(rel="stylesheet", href="https://cdnjs.cloudflare.com/ajax/libs/air-datepicker/2.2.3/css/datepicker.min.css")
    title landing-page
body
    header.landing-page__header
        +header 
    main.landing-page__main
        .landing-page-calendar
            <input type='text' class="datepicker-here" data-position="right top" />

    footer.landing-page__footer
        +footer
    submenu.landing-page__submenu
        +submenu